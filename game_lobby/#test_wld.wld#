test

Endpoint tester1;
Endpoint tester2;

Sequences {
  TestSeq: tester1.stuff -> tester2.doNothing;
}

Peered
{
    Text message;
}


Sequence TestSeq() {
    //tester2.didthis{}
    tester1.stuff {
        testfn();
    }
    tester2.doNothing {
       
    }
}


tester1 {
    Function (in: Nothing; returns: Nothing) testfn;
    onCreate(Function (in: Nothing; returns: Nothing) testing) {
        testfn = testing;
    }

    Public Function set_message(Text msg) {
        message = msg;
    }
    Public Function do_test() {
        TestSeq();
    }
}

